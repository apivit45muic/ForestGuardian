    border-bottom-right-radius: 70px;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    font-size: 100px;
    -unity-text-outline-color: rgb(0, 0, 0);
    background-color: rgb(44, 94, 44);
}
